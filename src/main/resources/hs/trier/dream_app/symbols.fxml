<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hs.trier.dream_app.controller.Symbols">
   <TableView fx:id="symbolsTableView" prefHeight="200.0" prefWidth="200.0"/>
   <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
      <TextField fx:id="symbolTextField" />
      <TextArea fx:id="symbolTextArea" prefHeight="200.0" prefWidth="200.0" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <Button onAction="#addSymbol" mnemonicParsing="false" text="Add Symbol" />
         <Button fx:id="editSymbolButton" mnemonicParsing="false" text="Edit Symbol" />
         <Button fx:id="deleteSymbolButton" mnemonicParsing="false" text="Delete Symbol" />
      </HBox>
   </VBox>
</HBox>
